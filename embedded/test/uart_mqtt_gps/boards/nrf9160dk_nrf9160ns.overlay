&uart1 {
	status = "okay";
	current-speed = <115200>;
	tx-pin = <11>;
	rx-pin = <10>;
	rts-pin = <12>;
	cts-pin = <13>;
};

&i2c2 {
	status = "disabled";
};

&uart2 {
	status = "okay";
	current-speed = <115200>;
	tx-pin = <24>;
	rx-pin = <23>;
	rts-pin = <26>;
	cts-pin = <27>;
};

&spi3 {

	status = "okay";
	sck-pin = <16>;
	mosi-pin = <18>;
	miso-pin = <17>;

	cs-gpios = <&gpio0 19 GPIO_ACTIVE_LOW>;

	sdhc0: sdhc@0 {
		compatible = "zephyr,mmc-spi-slot";
		reg = <0>;
		status = "okay";
		label = "SDHC0";
		spi-max-frequency = <24000000>;
	};
};