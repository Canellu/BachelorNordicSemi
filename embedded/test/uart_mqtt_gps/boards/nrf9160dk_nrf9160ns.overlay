&uart1 {
	status = "okay";
	current-speed = <115200>;
	tx-pin = <28>;
	rx-pin = <27>;
	rts-pin = <25>;
	cts-pin = <26>;
};

&i2c2 {
	status = "disabled";
};

&uart2 {
	status = "okay";
	current-speed = <115200>;
	tx-pin = <15>;
	rx-pin = <14>;
	rts-pin = <16>;
	cts-pin = <17>;
};

&spi3 {
   
    status = "okay";
    sck-pin = <10>;
    mosi-pin = <12>;
    miso-pin = <11>;

    cs-gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;

    sdhc0: sdhc@0 {
            compatible = "zephyr,mmc-spi-slot";
            reg = <0>;
            status = "okay";
            label = "SDHC0";
            spi-max-frequency = <24000000>;
    };
};