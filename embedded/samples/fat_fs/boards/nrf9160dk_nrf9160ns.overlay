&spi3 {
   
    status = "okay";
    sck-pin = <10>;
    mosi-pin = <12>;
    miso-pin = <11>;

    cs-gpios = <&gpio0 13 0>;

    sdhc0: sdhc@0 {
            compatible = "zephyr,mmc-spi-slot";
            reg = <0>;
            status = "okay";
            label = "SDHC0";
            spi-max-frequency = <24000000>;
    };
};