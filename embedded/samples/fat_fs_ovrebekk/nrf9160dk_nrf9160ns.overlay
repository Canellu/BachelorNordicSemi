&spi3 {
        status = "okay";
        cs-gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;
        sck-pin = <10>;
	mosi-pin = <12>;
	miso-pin = <11>;

        sdhc0: sdhc@0 {
                compatible = "zephyr,mmc-spi-slot";
                reg = <0>;
                status = "okay";
                label = "SDHC0";
                spi-max-frequency = <24000000>;
        };
};
