import 'package:flutter/material.dart';
import 'package:flutter/semantics.dart';

//Welcome page

void main () => runApp (MaterialApp(
  home: Scaffold(
    body: Container(
      foregroundDecoration: const BoxDecoration(
        image: DecorationImage(
          image: AssetImage('assets/logo.png'),
          fit: BoxFit.fill,
        ), //DecorationImage
      ), //BoxDecoration
    ), //Container
  ), //Scaffold
)); //MaterialApp
